<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://schemas.sportradar.com/sportsapi/v1/historicalmlb" xmlns="http://schemas.sportradar.com/sportsapi/v1/historicalmlb" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="../../includes/common/urn.xsd" />
    <xs:element name="season_mappings" type="seasonMappingsEndpoint" />
    <xs:complexType name="seasonMappingsEndpoint">
        <xs:sequence>
            <xs:element type="seasonMapping" name="mappings" />
        </xs:sequence>
        <xs:attribute type="xs:dateTime" name="generated_at"/>
    </xs:complexType>
    <xs:complexType name="seasonMapping">
        <xs:sequence>
            <xs:element name="mapping" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                    <xs:attribute name="us_id" type="xs:string"/>
                    <xs:attribute name="id" type="seasonUrn" />
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
</xs:schema>